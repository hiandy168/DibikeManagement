<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" type="text/css" href="./css/bootstrap.css">
        <link rel="stylesheet" type="text/css" href="./css/user.css">
        <script type="text/javascript" src="./js/jquery.min.js"></script>
        <script type="text/javascript" src="./js/html5shiv.js"></script>
        <script type="text/javascript" src="./js/size.js"></script>
        <script type="text/javascript" src="./js/ng/angular.min.js"></script>
       	<script type="text/javascript">
        	var vm=angular.module('userName',[]).run(function($rootScope){
        		$rootScope.userName = true;
        		$rootScope.userNoCheck = true;
        	});
        </script>
        <script type="text/javascript" src="./js/component/loginout.js"></script>
        <script type="text/javascript" src="./js/component/menu.js"></script>
       	<script type="text/javascript" src="./js/user/userNoCheck.js"></script>
	</head>
    <body ng-app='userName'>
        <header class="header" ng-include="'./component/header.html'" ng-controller='logList'></header>
        <section class="main">
            <div class="container">
                <aside class="menu_box" ng-include="'./component/menu.html'" ng-controller='menuList'></aside>
                <section class="section-box" ng-controller="userList">
                    <div class="mp_news__box">
                        <div class="title_bar" ng-bind="'实名认证'"></div>
                        <div class="tabNav" ng-include="'./component/userNav.html'"></div>
						<table class="uname_bar">
							<thead ng-include="'./component/userthead.html'"></thead>
							<tbody>
								<tr ng-repeat="data in queryNoCheck track by $index">
									<td ng-bind="data.name"></td>
									<td ng-bind="data.nationality"></td>
									<td ng-bind="data.createDate"></td>
									<td>
										<input type="button" ng-value="'审核'" ng-click="audit(data.memberID)"/>
									</td>
								</tr>
							</tbody>
						</table>
					</div>
                </section>
            </div>
        </section>
        <footer class="footer" ng-include="'./component/footer.html'"></footer>
    </body>
</html>