<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" type="text/css" href="./css/bootstrap.css">
        <link rel="stylesheet" type="text/css" href="./css/user.css">
        <script type="text/javascript" src="./js/jquery.min.js"></script>
        <script type="text/javascript" src="./js/jquery.page.js"></script>
        <script type="text/javascript" src="./js/html5shiv.js"></script>
        <script type="text/javascript" src="./js/size.js"></script>
        <script type="text/javascript" src="./js/ng/angular.min.js"></script>
       	<script type="text/javascript">
        	var vm=angular.module('inquireApp',[]).run(function($rootScope){
        		$rootScope.inquire= true;
        	});
        </script>
        <script type="text/javascript" src="./js/component/loginout.js"></script>
        <script type="text/javascript" src="./js/component/menu.js"></script>
        <script type="text/javascript" src="./js/other/inquire.js"></script>
	</head>
    <body ng-app='inquireApp'>
        <header class="header" ng-include="'./component/header.html'" ng-controller='logList'></header>
        <section class="main">
            <div class="container">
                <aside class="menu_box" ng-include="'./component/menu.html'" ng-controller='menuList'></aside>
                <section class="section-box" ng-controller="inquireList">
                    <div class="mp_news__box">
                        <div class="title_bar" ng-bind="'查询功能'"></div>
                        <textarea class="inquireText"></textarea>
                        <input type="button" class="save" ng-value="'提交'" ng-click="submit()">
                        <ul>
							<li></li>
						</ul>
						<div ng-include="'./component/page.html'"></div>
					</div>
                </section>
            </div>
        </section>
        <footer class="footer" ng-include="'./component/footer.html'"></footer>
    </body>
</html>