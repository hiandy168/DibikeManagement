<dl class="menu" ng-repeat="data in dataList">
    <dt class="menu_title">
        <span class="glyphicon glyphicon-th-large"></span>
        <span ng-bind="data[0]"></span>
    </dt>
    <dd class="menu_item" ng-repeat="list in data[1]" ng-class="{true:'selected'}[{{list[1].split('.')[0]}}]">
        <a ng-href="./{{list[1]}}" ng-bind="list[0]" ng-class="{true:'selected'}[{{list[1].split('.')[0]}}]"></a>
    </dd> 
</dl>

